    "material-ui-dropzone": "^3.5.0",
    "png-js": "^1.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.12.2",
    "@types/node": "^14.14.31",
    "@types/react": "^16.9.49",
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "next": "^11.0.1",
    "nextron": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.3.5"
