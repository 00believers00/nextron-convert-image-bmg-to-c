{
  "private": true,
  "name": "convert-image",
  "description": "Bitmap 24 bit to 16bit out .c file",
  "version": "1.0.0",
  "author": "Atiwat Promarin <np.nawin@gmail.com>",
  "main": "app/background.js",
  "scripts": {
    "dev": "nextron",
    "build": "NODE_ENV=production nextron build",
    "postinstall": "electron-builder install-app-deps",
    "build:all": "nextron build --all",
    "build:win32": "NODE_ENV=production nextron build --win --ia32",
    "build:win64": "NODE_ENV=production nextron build --win --x64",
    "build:mac": "NODE_ENV=production nextron build --mac --x64",
    "build:mac-arm64": "NODE_ENV=production nextron build --mac --arm64",
    "build:linux": "NODE_ENV=production nextron build --linux --arm64"
  },
  "dependencies": {
    "@material-ui/icons": "^4.11.2",
